# Deployment URL (Vercel URL)
NEXT_PUBLIC_DEPLOY_URL=https://your-app.vercel.app

# İkas OAuth Credentials (from İkas Partner Portal)
NEXT_PUBLIC_CLIENT_ID=your-ikas-client-id
CLIENT_SECRET=your-ikas-client-secret

# İkas API URLs
NEXT_PUBLIC_GRAPH_API_URL=https://api.myikas.com/api/admin/graphql
NEXT_PUBLIC_ADMIN_URL=https://api.myikas.com

# Session Secret (Generate a random 32-character string)
# You can generate one here: https://1password.com/password-generator/
SECRET_COOKIE_PASSWORD=replace-this-with-a-random-32-character-string

# Database (PostgreSQL - use Supabase, Neon, or any PostgreSQL provider)
# Example: postgresql://user:password@host:5432/database?schema=public
DATABASE_URL=postgresql://user:password@host:5432/database
